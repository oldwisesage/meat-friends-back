{
  "name": "meat-friends-back",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "keystone-next",
    "seed-data": "keystone-next --seed-data",
    "reset-db": "keystone-next dev --reset-db",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "eslintConfig": {
    "extends": "wesbos/typescript.js",
    "rules": {
      "@typescript-eslint/no-unsafe-assignment": 0,
      "require-await": "off",
      "@typescript-eslint/require-await": "error",
      "prettier/prettier": {
        "singlequotes": true
      }
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 10,
            "browsers": [
              "last 2 chrome versions",
              "last 2 firefox versions",
              "last 2 safari versions",
              "last 2 edge versions"
            ]
          }
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@keystone-next/auth": "^27.0.0",
    "@keystone-next/cloudinary": "^6.0.0",
    "@keystone-next/fields": "^11.0.0",
    "@keystone-next/keystone": "^20.0.0",
    "@keystone-next/types": "^20.0.0",
    "@keystonejs/server-side-graphql-client": "^2.1.2",
    "@prisma/client": "^2.25.0",
    "dotenv": "^10.0.0",
    "next": "11.0.1",
    "nodemailer": "^6.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "stripe": "^8.156.0"
  },
  "devDependencies": {
    "@types/node": "^14.17.4",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-env": "^21.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-wesbos": "^1.0.1",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.1",
    "prisma": "^2.25.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
